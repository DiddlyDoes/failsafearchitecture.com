                            <button 
                                onClick={onVerify} 
                                className="w-full py-4 text-xs font-bold bg-white text-black hover:bg-zinc-200 transition-colors tracking-widest uppercase"
                            >
                                > Affirmative (I am 18+)
                            </button>
